{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": true,
		"removeComments": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"target": "ESNext",
		"sourceMap": false,
		"outDir": "./dist",
		"baseUrl": "./",
		"incremental": true,
		"resolveJsonModule": true,
		"strict": true
	}
}